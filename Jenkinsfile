pipeline{
    agent any
    stages{
        stage("compile"){
            steps{
                script{
                    echo "compiling the code"
                }
            }
        }
        stage("unitTest"){
            steps{
                script{
                    echo "Testing the code"
                }
            }
        }
        stage("package"){
            steps{
                script{
                    echo "Packaging the code"
                }
            }
        }
    }
}
